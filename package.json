{
  "name": "mediapackage-mock",
  "version": "1.0.0",
  "description": "AWS MediaPackage Mock Server for HLS WebDAV testing",
  "main": "dist/server.js",
  "scripts": {
    "start": "ts-node src/server.ts",
    "build": "tsc",
    "dev": "nodemon --exec ts-node src/server.ts",
    "test": "jest",
    "test:watch": "jest --watch",
    "loglevel": "node set-loglevel.js",
    "loglevel:debug": "node set-loglevel.js debug",
    "loglevel:info": "node set-loglevel.js info",
    "loglevel:warn": "node set-loglevel.js warn",
    "loglevel:error": "node set-loglevel.js error"
  },
  "dependencies": {
    "@types/axios": "^0.9.36",
    "@types/js-yaml": "^4.0.9",
    "axios": "^1.8.4",
    "express": "^4.18.2",
    "js-yaml": "^4.1.0",
    "winston": "^3.17.0"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.12",
    "@types/node": "^20.11.19",
    "jest": "^29.7.0",
    "nodemon": "^3.0.3",
    "ts-jest": "^29.1.2",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.3"
  }
}
